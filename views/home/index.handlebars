<section id="top_decks">
    <ol>
        {{#each decks}}
        <li>
            <article class="deck">
                <a href="/decks/{{this._id}}">{{this.name}}</a>
                <p>Rating: {{this.rating}}</p>
                <p>{{this.description}}</p>
            </article>
        </li>
        {{/each}}
    </ol>
</section>
